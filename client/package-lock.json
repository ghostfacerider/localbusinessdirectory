{
  "name": "client2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1.7.9",
    "cra-template-typescript": "1.2.0",
    "framer-motion": "^11.15.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "react-scripts": "5.0.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.6.0",
    "@types/node": "^22.12.0",
    "@types/react": "^19.0.3",
    "@types/react-dom": "^19.0.3",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^9.1.0",
    "typescript": "^5.8.0",
    "update-browserslist-db": "^1.1.0",
    "web-vitals": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "prettier:check": "prettier \"src/**/*.{ts,tsx,js,jsx}\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "npm start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-typescript": "^20.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jest": "^30.0.0",
    "eslint-plugin-jsx-a11y": "^7.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^8.0.0",
    "eslint-plugin-react-hooks": "^6.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "^3.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "npm run prettier:fix .",
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}